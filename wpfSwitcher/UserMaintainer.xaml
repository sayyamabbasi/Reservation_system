                                <DataGridTextColumn x:Name="ttl" Width="230" Header="bill" Binding="{Binding total }"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                </Grid>
            </TabItem>

        </TabControl>
    </Grid>
</UserControl>
